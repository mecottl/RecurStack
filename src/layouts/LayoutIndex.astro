import HeaderRecurso from "../components/HeaderRecurso.astro";
import "../styles/index.css";
interface Props {
	title: string;
}
const { title } = Astro.props;
---

<header class="navbar">
    <div class="navleft">
        <img src="../../logo.svg" alt="RecurStack Logo" />
    </div>

    <div class="navcenter">
        <div class="recursos">
        <HeaderRecurso href="/Recursos/HTML"> HTML </HeaderRecurso>
        <HeaderRecurso title="CSS" href="/Recursos/CSS"> CSS </HeaderRecurso>
        <HeaderRecurso href="/Recursos/JS"> JS </HeaderRecurso>
        <HeaderRecurso href="/Recursos/SQL"> SQL </HeaderRecurso>
        <HeaderRecurso href="/Recursos/Icons"> SVG </HeaderRecurso>
        <HeaderRecurso href="/Recursos/Mas"> Mas </HeaderRecurso>
        <input type="text" placeholder="Buscar..." class="search-box" />
        </div>

    </div>
</header>
<main>
    <slot />
</main>